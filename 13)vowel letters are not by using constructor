class hello{
    char ch;
    void input(char ch){
        
        this.ch=ch;
         if(ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u' )
            System.out.println(ch + " is vowel");
        else
            System.out.println(ch + " is consonant");

    }
}
   
public class Main
{
	public static void main(String[] args) {
	
	hello a=new hello();
	a.input('a');
	a.input('b');
	
	}
}
