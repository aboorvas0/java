class gcd{
    int n1,n2;
    int gcd=1;
    void gcd1(int n1,int n2)
    {
        this.n1=n1;
        this.n2=n2;
       for (int i = 1; i <= n1 && i <= n2; ++i) 
       {
            if (n1 % i == 0 && n2 % i == 0)
                 gcd = i;
       }
       display();
       
    }
    void display(){
         System.out.println("GCD of " + n1 +" and " + n2 + " is " + gcd);
    }
}
public class Main
{
	public static void main(String[] args) {
	 gcd a=new gcd();
	 a.gcd1(23,33);
  }
}
	

